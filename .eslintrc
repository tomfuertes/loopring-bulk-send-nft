{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": "off",
    "typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-explicit-any": "off",
    // Remember, this means error!
    "camelcase": ["warn"]
  }
}
